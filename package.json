{
  "name": "tailwind-connect-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo build",
    "dev": "dotenv -- turbo dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "storybook": "turbo run storybook",
    "test": "turbo run test -- --runInBand",
    "clean": "rm -rf node_modules && turbo run clean",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "dotenv-cli": "latest",
    "turbo": "^1.10.13"
  },
  "packageManager": "pnpm@8.15.1",
  "pnpm": {
    "patchedDependencies": {
      "@cosmos-kit/core@2.7.2": "patches/@cosmos-kit__core@2.7.2.patch",
      "@chain-registry/types@0.17.0": "patches/@chain-registry__types@0.17.0.patch"
    }
  }
}
