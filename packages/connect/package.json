{
  "name": "@tailwindzone/connect",
  "version": "2.4.3",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "pnpm tsc -p tsconfig.json --outDir dist/cjs --module commonjs",
    "build:esm": "pnpm tsc -p tsconfig.json --outDir dist/esm --module es2022",
    "clean": "rm -rf dist && rm -rf node_modules",
    "preinstall": "npx only-allow pnpm"
  },
  "author": "TAILWIND Labs, Inc.",
  "license": "ISC",
  "description": "Simple vanilla JS wallet connect for Tailwind wallet.",
  "dependencies": {
    "@keplr-wallet/types": "^0.12.63"
  },
  "devDependencies": {
    "@types/long": "4.0.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.1"
}
