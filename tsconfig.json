{
  "compilerOptions": {
    "jsx": "react",
    "skipLibCheck": true,
    "emitDeclarationOnly": false,
    "declaration": true,
    "esModuleInterop": true,
    "target": "es2022",
    "module": "es2022",
    "lib": [
      "es2022",
      "DOM"
    ],
    "sourceMap": false,
    "isolatedModules": true,
    "allowJs": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}